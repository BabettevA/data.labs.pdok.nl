#
# Datasets
#
dataset:Monumenten a void:Dataset;
  rdfs:label "Monumenten";
  void:classPartition [ void:class ceo:Rijksmonument];
  void:sparqlEndpoint <https://data.pdok.nl/sparql/rce>;
  dct:publisher organisatie:RCE;
  void:exampleResource <https://linkeddata.cultureelerfgoed.nl/id/ceo/rijksmonument/516110>;
.
<https://linkeddata.cultureelerfgoed.nl/id/ceo/rijksmonument/516110> a ceo:Rijksmonument.

dataset:KadastraleKaart a void:Dataset;
  rdfs:label "Kadastrale kaart";
  void:classPartition [ void:class brk:Perceel, brk:KadastraleGemeente];
  void:sparqlEndpoint <https://data.pdok.nl/sparql>;
  dct:publisher organisatie:Kadaster;
  void:example <http://brk.basisregistraties.overheid.nl/id/perceel/420127269>;
  void:example <http://brk.basisregistraties.overheid.nl/id/begrip/EHV00>;
.
<http://brk.basisregistraties.overheid.nl/id/perceel/420127269> a brk:Perceel.
<http://brk.basisregistraties.overheid.nl/id/begrip/EHV00> a brk:KadastraleGemeente.

dataset:Brt a void:Dataset;
  rdfs:label "Basisregistratie Topografie";
  void:classPartition [ void:class brt:Gebouw];
  void:sparqlEndpoint <https://data.pdok.nl/sparql>;
  dct:publisher organisatie:Kadaster;
.

dataset:Bag a void:Dataset;
  rdfs:label "Basisregistratie adressen en gebouwen";
  void:classPartition [ void:class bag:Pand, bag:Nummeraanduiding];
  void:sparqlEndpoint <https://data.pdok.nl/sparql>;
  dct:publisher organisatie:Kadaster;
.

dataset:WoonplaatsGemeente a void:Dataset;
  rdfs:label "Linkset Bag woonplaats - BRT gemeente";
  void:target dataset:Monumenten;
  void:sparqlEndpoint <https://data.labs.pdok.nl/sparql>;
  rdfs:comment "Deze data zit nu in een waardelijst. Deze kan vrij makkelijk op labs gezet worden";
  dct:publisher organisatie:Kadaster;
.

dataset:PandGebouw a void:Dataset;
  rdfs:label "Linkset Bag pand - BRT gebouw";
  void:target dataset:Bag;
  void:target dataset:Brt;
  void:sparqlEndpoint <https://data.labs.pdok.nl/sparql>;
  rdfs:comment "Deze data zit nu in een waardelijst. Deze kan vrij makkelijk op labs gezet worden";
  dct:publisher organisatie:Kadaster;
.

dataset:PerceelFiliatie a void:Dataset;
  rdfs:label "Perceel filiatie";
  void:target dataset:KadastraleKaart;
  void:sparqlEndpoint <https://data.labs.pdok.nl/sparql>;
  rdfs:comment "Deze data is nu niet beschikbaar. We hebben njson logbestand, maar die lijkt niet voldoende informatie te bevatten";
  dct:publisher organisatie:Kadaster;
.

dataset:LKO a void:Dataset;
  rdfs:label "LKO - Koppeling Brk perceel - Bag nummeraanduiding";
  void:target dataset:KadastraleKaart;
  void:target dataset:Bag;
  void:sparqlEndpoint <https://data.labs.pdok.nl/sparql>;
  rdfs:comment "Deze data is nog niet beschikbaar. Er is intern een bestand beschikbaar, dit is nog niet omgezet (maar zou wel op te pakken kunnen zijn)";
  dct:publisher organisatie:Kadaster;
.

dataset:GemeenteGemeenteGebied a void:Dataset;
  rdfs:label "Bbi gemeente Brk Gemeentegebied";
  void:target [void:classPartition [void:class brk:KadastraleGemeente]];
  void:target [void:classPartition [void:class brk:Gemeentegebied]];
  void:sparqlEndpoint <https://data.labs.pdok.nl/sparql>;
  dct:publisher organisatie:Kadaster;
  rdfs:comment "NOG EVEN UITZOEKEN: Mogelijk staat deze data al op labs?";
.

dataset:RijksmonumentPerceel a void:Dataset;
  rdfs:label "Monumentpercelen"@nl;
  void:target [void:classPartition [void:class ceo:Rijksmonument]];
  void:target [void:classPartition [void:class ceo:Perceel]];
  void:linkPredicate ceo:heeftPerceel;
  void:sparqlEndpoint <https://data.pdok.nl/sparql/rce>;
  dct:publisher organisatie:RCE;
.

dataset:PerceelAdres a void:Dataset;
  rdfs:label "BRK-BAG Perceel adres linkset";
  void:target [void:classPartition [void:class brk:Perceel]];
  void:target [void:classPartition [void:class bag:AdresseerbaarObject]];
  void:sparqlEndpoint <https://data.labs.pdok.nl/sparql>;
  dct:publisher organisatie:Kadaster;
  void:exampleResource <http://bag.basisregistraties.overheid.nl/bag/id/verblijfsobject/0772010000746331>;
.
<http://bag.basisregistraties.overheid.nl/bag/id/verblijfsobject/0772010000746331> a bag:AdresseerbaarObject.

dataset:Bbi a void:Dataset;
  rdfs:label "BBI";
  void:sparqlEndpoint <https://data.labs.pdok.nl/sparql>;
  rdfs:comment "Dit staat op labs, nog wel de classPartition toevoegen!";
